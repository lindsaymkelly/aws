<!DOCTYPE html>
<html>
  <head>
    <title>AcademicWorkshop</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <% if @errors%>
      <div>
        <ul>
        <% @errors.each do |e| %>
          <li><%= e %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
    <%= yield %>
  </body>
</html>
